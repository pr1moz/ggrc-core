<h6>{{type}} objects</h6>
{{#mapped_objects}}
  <ul class="attachment-list objects-list">
    {{#each mapped_objects}}
      <li>
        <i class="fa fa-{{icon}} color"></i>
        {{title}}
      </li>
    {{/each}}
  </ul>
{{/mapped_objects}}
