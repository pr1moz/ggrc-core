<h6>
  Workflow
  <span class="required">*</span>
</h6>
{{#mapped_workflow}}
  <ul class="attachment-list objects-list">
    {{#each mapped_workflow}}
      <li>
        <i class="fa fa-{{icon}}"></i>
        {{title}}
        <a class="edit-attachment-item" href="javascript://">
          <i class="fa fa-pencil-square-o"></i>
        </a>
      </li>
    {{/each}}
  </ul>
{{/mapped_workflow}}
