<h6>
  Workflow
  <span class="required">*</span>
</h6>
{{#mapped_workflow}}
  <ul class="attachment-list objects-list">
    {{#each mapped_workflow}}
      <li>
        <i class="fa fa-{{icon}}"></i>
        {{title}}
        {{^read_only}}
          <a class="edit-attachment-item" href="#WorkflowMapper" rel="tooltip" data-placement="left" data-toggle="modal"
            data-original-title="Map Workflow">
            <i class="fa fa-pencil-square-o"></i>
          </a>
        {{/read_only}}
      </li>
    {{/each}}
  </ul>
{{/mapped_workflow}}
