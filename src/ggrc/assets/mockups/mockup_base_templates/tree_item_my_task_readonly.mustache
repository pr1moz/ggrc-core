<div class="item-main {{#if active}}active{{/if}}" data-model="true">
  <div class="item-wrap">
    <div class="openclose"></div>
    <div class="select">
      <div class="item-data">
        <div class="row-fluid">
          <div class="span4">
            <div class="title tree-title-area" rel="tooltip" data-placement="left" title=""
            data-original-title="{{title}}">
              <i class="fa fa-{{icon}}"></i>
              {{title}}
            </div>
          </div>

          <div class="span2">
            <div class="row-fluid">
              <div class="span12">
                <div class="owner tree-title-area">
                  {{workflow_title}}
                </div>
              </div>
            </div>
          </div>

          <div class="span2">
            <div class="row-fluid">
              <div class="span12">
                <div class="owner tree-title-area">
                  {{obj_title}}
                </div>
              </div>
            </div>
          </div>

          <div class="span4">
            <ul class="tree-action-list tasks-action">
              <li>
                <div class="date">
                  <span rel="tooltip" data-placement="top" data-original-title="Ends on">
                    <span class="uppercase">Ends on</span> 20th
                  </span>
                </div>
              </li>
            </ul>
          </div>
        </div>
      </div><!-- item-data end -->
    </div><!-- select end -->
  </div><!-- item-wrap end -->
</div><!-- item-main end -->

{{#if children}}
  <div class="tier-2-info item-content">
    <div class="tier-2-info-content">
      <div class="inner-tree">
        <ul class="tree-structure new-tree"></ul>
      </div>
    </div>
  </div>
{{/if}}
